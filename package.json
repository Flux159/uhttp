{
  "name": "uhttp",
  "description": "A micro ajax http library with a unified api for browsers and nodejs",
  "version": "1.1.3",
  "repository": "https://github.com/Flux159/uhttp",
  "homepage": "http://flux159.github.io/uhttp/",
  "keywords": [
    "ajax",
    "http",
    "nodejs",
    "client"
  ],
  "scripts": {
    "test": "NODE_ENV=test node node_modules/.bin/mocha \"test/**/*.js\"",
    "test-coverage": "NODE_ENV=test node node_modules/.bin/mocha -r blanket -R html-cov > coverage/coverage.html"
  },
  "devDependencies": {
    "mocha": "^2.2.1",
    "jsdom": "^3.1.2",
    "grunt": "~0.4.1",
    "grunt-shell": "^0.7.0",
    "grunt-banner": "^0.3.1",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-jshint": "~0.7.1",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-yuidoc": "^0.7.0",
    "jshint-stylish": "~0.1.3",
    "express": "~4.12.3",
    "morgan": "~1.0.0",
    "body-parser": "~1.0.0",
    "method-override": "~1.0.0",
    "chai": "^2.2.0",
    "blanket": "^1.1.6",
    "connect-multiparty": "^1.2.5"
  },
  "config": {
    "blanket": {
      "pattern": [
        "src/**/*.js"
      ]
    }
  }
}
